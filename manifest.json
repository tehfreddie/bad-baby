{
  "manifest_version": 2,

  "name": "Brexit means Breadsticks",
  "version": "0.1.4",
  "description": "Turns every mention of Brexit on any website into 'breadsticks'",
  "icons": { "16": "images/breadsticks16.png",
           "48": "images/breadsticks48.png",
          "128": "images/breadsticks128.png" },
  "content_scripts": [
    {
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "run_at":"document_end"

  }
]

}
