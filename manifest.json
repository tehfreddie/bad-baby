{
  "manifest_version": 2,

  "name": "bad babies",
  "version": "0.1.4",
  "description": "Turns every mention of enfant terrible on any website into 'bad baby'",
  "icons": { "16": "images/badbaby16.png",
           "48": "images/badbaby48.png",
          "128": "images/badbaby128.png" },
  "content_scripts": [
    {
    "matches": ["*://*/*"],
    "js": ["content.js"],
    "run_at":"document_end"

  }
]

}
